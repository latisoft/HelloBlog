<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
  <p class="info">
    作者i：<a href="/u/<%= post.name %>"><%= post.name %></a> |
    日期i：<%= post.time.minute %>
  </p>
  <p><%- post.post %></p>
<% }) %>
<%- include paging %>
<%- include footer %>